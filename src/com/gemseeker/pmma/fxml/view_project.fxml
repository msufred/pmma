<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="contentView" fx:id="container" alignment="TOP_CENTER" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #cccccc;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="toolbarLeft" alignment="CENTER_LEFT" spacing="8.0" styleClass="menu-panel">
         <children>
            <Label fx:id="projectName" styleClass="title-big" text="Project Name" />
            <Label fx:id="statusLabel" styleClass="title-big-light" text="(Status)" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
      <HBox fx:id="optionsPane" alignment="CENTER" prefHeight="56.0" prefWidth="200.0" spacing="16.0" />
      <StackPane fx:id="stackPane" prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </StackPane>
   </children>
   <stylesheets>
      <URL value="@../css/styles.css" />
      <URL value="@../css/controls.css" />
   </stylesheets>
</VBox>
